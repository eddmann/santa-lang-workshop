--TEST--
evaluator: collections_merge_and_set_rules
--FILE--
// Pushing a dictionary into a set is allowed
let s = {1};
let s2 = push(#{"a":1}, s);
puts(size(s2));

// Right-biased dictionary merge
puts(#{"a":1} + #{"a":2, "b":3});

// Set union and List concat
puts({1,2} + {2,3});
puts([1,2] + [3]);
--EXPECT--
2 
#{"a": 2, "b": 3} 
{1, 2, 3} 
[1, 2, 3] 
nil